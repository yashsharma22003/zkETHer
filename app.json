{
  "expo": {
    "name": "mobile-zkETHer",
    "slug": "mobile-zkether",
    "version": "1.0.0",
    "orientation": "portrait",
    "icon": "./assets/icon.png",
    "userInterfaceStyle": "light",
    "splash": {
      "image": "./assets/splash-icon.png",
      "resizeMode": "contain",
      "backgroundColor": "#ffffff"
    },
    "assetBundlePatterns": [
      "**/*"
    ],
    "ios": {
      "supportsTablet": true,
      "infoPlist": {
        "LSApplicationQueriesSchemes": [
          "metamask",
          "trust",
          "safe",
          "rainbow",
          "uniswap",
          "coinbase",
          "exodus",
          "phantom"
        ]
      },
      "bundleIdentifier": "com.anonymous.mobilezkether"
    },
    "android": {
      "adaptiveIcon": {
        "foregroundImage": "./assets/adaptive-icon.png",
        "backgroundColor": "#ffffff"
      },
      "intentFilters": [
        {
          "action": "VIEW",
          "data": [
            {
              "scheme": "zkether"
            }
          ],
          "category": [
            "BROWSABLE",
            "DEFAULT"
          ]
        }
      ],
      "package": "com.anonymous.mobilezkether"
    },
    "web": {
      "favicon": "./assets/favicon.png"
    },
    "scheme": "zkether",
    "plugins": [
      "expo-secure-store",
      "expo-asset"
    ]
  }
}
